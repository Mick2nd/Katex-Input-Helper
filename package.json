{
  "name": "joplin-plugin-katex-input-helper",
  "version": "1.0.10",
  "scripts": {
    "dist": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtraScripts && webpack --env joplin-plugin-config=createArchive",
    "prepare": "npm run dist",
    "updateVersion": "webpack --env joplin-plugin-config=updateVersion",
    "update": "npm install -g generator-joplin && yo joplin --node-package-manager npm --update --force",
    "build-docs": "typedoc --options typedoc.json && npm exec --package=@atao60/fse-cli fse copy ./img ./docs/img",
    "test": "vitest --root './tests' --run --reporter verbose --environment jsdom",
    "jtest": "jest"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "files": [
    "publish"
  ],
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.9",
    "@joplin/lib": "^2.6.3",
    "@types/jquery": "^3.5.32",
    "@types/node": "^18.7.13",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "easyui": "^8.0.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "jest": "^29.7.0",
    "path": "^0.12.7",
    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "typedoc": "^0.26.10",
    "typedoc-umlclass": "^0.10.0",
    "typescript": "^4.8.2",
    "vitest": "^3.1.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "eslintConfig": {
    "plugins": [
      "de.habelt-jena.KatexInputHelper"
    ],
    "env": {
      "browser": true,
      "node": true,
      "de.habelt-jena.KatexInputHelper/custom": true
    }
  }
}
