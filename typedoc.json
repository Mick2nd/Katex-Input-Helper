{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["src", "img", "README.md"],
	"entryPointStrategy": "expand",
	"out": "docs",
	"cleanOutputDir": true,
	
	"name": "Katex Input Helper",
	"theme": "default", 
	"basePath": ".",
	"skipErrorChecking": true,
	"validation": {
	    "notExported": true,
	    "invalidLink": true,
	    "notDocumented": false,
	},
	"logLevel": "Error",
    "plugin": ["typedoc-umlclass"],
    "umlClassDiagram": 
    {
        "type": "detailed",
        "location": "local",
        "format": "svg"
    }
}
